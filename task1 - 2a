#include <iostream>
using namespace std;

int main()
{
	int a;
	cin >> a;
	if (a == 12 || a == 1 || a == 2)
	{
		cout << "0, is Winter";
	}
	if (a == 3 || a == 4 || a == 5)
	{
		cout << "1, is Spring";
	}
	if (a == 6 || a == 7 || a == 8)
	{
		cout << "2, is Summer";
	}
	if (a == 9 || a == 10 || a == 11)
	{
		cout << "3, is Autumn";
	}


	return 0;
}
