#include <iostream>
#include "stdafx.h"
#include <cmath>
using namespace std;

int main()
{
	double a;
	double b;
	double c;
	double x_1;
	double x_2;
	double D;
	
	cout << "set a:" << endl;
	cin >> a;
	cout << "set b:" << endl;
	cin >> b;
	cout << "set c:" << endl;
	cin >> c;

	if (a == 0)
	{
		x_1 = -(c / b);
		cout << "X=" << x_1 << endl;
	}
	else
	{
		D = b*b - 4*a*c;
		if (D >= 0)
		{
			if (D == 0)
			{
				x_1 = ((-b) + sqrt(D)) / (2 * a);
				cout << x_1 << endl;
			}
			else
			{
				x_1 = ((-b) + sqrt(D)) / (2 * a);
				x_2 = ((-b) - sqrt(D)) / (2 * a);
				cout << "X1=" << x_1 << endl << "X2=" << x_2 << endl;
			}
		}
		else
		{
			cout << "D<0" << endl;
		}
	}
}
