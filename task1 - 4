#include <iostream>
#include "stdafx.h"
using namespace std;


int main()
{
	int number;
	int f_half = 0;
	int s_half = 0;

	cout << "set number:";
	cin >> number;

	while (number > 999)
	{
		s_half += number % 10;
		number /= 10;
	}

	while (number > 0)
	{
		f_half += number % 10;
		number /= 10;
	}

	if (f_half == s_half)
	{
		cout << "yes";
	}
	else
	{
		cout << "no";
	}
}
